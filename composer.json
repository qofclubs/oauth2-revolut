{
    "name": "vdbelt/oauth2-revolut",
    "description": "Revolut Provider for OAuth 2.0 Client",
    "license": "MIT",
    "authors": [
        {
            "name": "Martin van de Belt",
            "email": "martin@vandebelt.dk"
        }
    ],
    "require": {
        "league/oauth2-client": "^2.4.1",
        "lcobucci/jwt": "^4.0|^5.0"
    },
    "autoload": {
        "psr-4": {
            "League\\OAuth2\\Client\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "League\\OAuth2\\Client\\Test\\": "test/src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5 || ^8.0 || ^9.0",
        "mockery/mockery": "^1.3",
        "ext-json": "*",
        "squizlabs/php_codesniffer": "^3.5"
    }
}
